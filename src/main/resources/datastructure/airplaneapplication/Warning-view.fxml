<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="295.0" prefWidth="411" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="datastructure.airplaneapplication.WarningController">
    <children>
        <Label layoutX="120.0" text="Warning" textFill="#ae5353">
            <font>
                <Font name="Adelle Sans Devanagari Semibold" size="48.0" />
            </font>
        </Label>
        <Label fx:id="error" alignment="CENTER" layoutX="20.0" layoutY="191.0" prefHeight="57.0" prefWidth="371.0" textAlignment="CENTER" textFill="#ae5353" wrapText="true">
            <font>
                <Font name="Adelle Sans Devanagari Semibold" size="16.0" />
            </font></Label>
        <ImageView fitHeight="104.0" fitWidth="117.0" layoutX="154.0" layoutY="85.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../../../../../../Downloads/Untitled%20design-2.png" />
            </image>
        </ImageView>
      <Button fx:id="confirm" layoutX="114.0" layoutY="252.0" mnemonicParsing="false" onAction="#onConfirm" prefHeight="34.0" prefWidth="81.0" text="Confirm" />
      <Button fx:id="cancel" layoutX="212.0" layoutY="252.0" mnemonicParsing="false" onAction="#onCancel" prefHeight="34.0" prefWidth="81.0" text="Cancel" />
    </children>

</AnchorPane>
