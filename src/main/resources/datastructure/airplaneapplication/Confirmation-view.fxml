<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="264.0" prefWidth="431.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="datastructure.airplaneapplication.ConfirmationController">
   <children>
      <Label layoutX="70.0" text="Confirmation" textFill="#ae5353">
         <font>
            <Font name="Adelle Sans Devanagari Semibold" size="48.0" />
         </font>
      </Label>
      <Label layoutX="60.0" layoutY="141.0" text="Enter Confirmation Code:" />
      <TextField fx:id="codeee" layoutX="201.0" layoutY="136.0" />
      <Button fx:id="reSend" layoutX="216.0" layoutY="199.0" mnemonicParsing="false" onAction="#onReSend" text="Re-Send Code" textFill="#ae5353">
         <font>
            <Font name="Adelle Sans Devanagari Regular" size="13.0" />
         </font>
      </Button>
      <Button fx:id="submit" layoutX="104.0" layoutY="199.0" mnemonicParsing="false" onAction="#onSubmit" text="Submit Code" textFill="#ae5353">
         <font>
            <Font name="Adelle Sans Devanagari Regular" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
